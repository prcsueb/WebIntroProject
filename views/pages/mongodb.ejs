<!DOCTYPE html>
<html>
<head>
</head>
<body>
<div class="container">
    <h2>User table detail</h2>
    <ul>
        <table  border="2">
            <tr>
                <td>Id</td>
                <td>Name</td>
            </tr>
        <% results.forEach(function(r) { %>
            <tr>
                <td><%= r.id %></td>
                <td><%= r.name %></td>
            </tr>
            <!--<li><%= r.id %> - <%= r.name %></li>-->
        <% }); %>
        </table>
    </ul>
</div>
</body>
</html>